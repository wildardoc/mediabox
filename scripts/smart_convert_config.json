{
    "max_cpu_percent": 75,
    "max_memory_percent": 90,
    "max_load_average": 8.0,
    "min_available_memory_gb": 4,
    "check_interval": 30,
    "max_parallel_jobs": 4,
    "min_parallel_jobs": 1,
    "plex_priority": true,
    "download_priority": true,
    "zfs_aware": true,
    "target_directories": [
        "/Storage/media/movies",
        "/Storage/media/tv"
    ],
    "pause_for_processes": [
        "PlexTranscoder",
        "plex_ffmpeg",
        "import.sh",
        "media_update.py"
    ],
    "video_extensions": [
        "mkv", "mp4", "avi", "mov", "wmv", "flv"
    ]
}
